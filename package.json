{
  "name": "shorturl-app",
  "version": "1.0.0",
  "description": "Short URL Client + Server",
  "proxy": "http://localhost:8080",
  "scripts": {
    "test": "npm run test",
    "build": "npm run build:server && npm run build:webapp",
    "build:server": "tsc -p server",
    "oldstart:server": "nodemon --watch \"server/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node --project server/tsconfig.json server/index.ts",
    "start:server": "ts-node-dev --project server/tsconfig.json -- server/index.ts",
    "prestart:prod": "npm run build:all",
    "start": "cd ./dist && node ./index.js",
    "start:webapp": "react-scripts start",
    "build:webapp": "react-scripts build",
    "postbuild:webapp": "mkdir -p ./dist/ && rm -rf ./dist/webapp && mv ./build ./dist/webapp",
    "test:webapp": "react-scripts test",
    "eject:webapp": "react-scripts eject",
    "build:all": "npm run build:server && npm run build:webapp",
    "docker:build": "docker build . -t zoom2ashish/shorturl-app",
    "ts-node-dev": "ts-node-dev --project server/tsconfig.json ./server/index.ts"
  },
  "author": "Ashish Patel",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.8.4",
    "@material-ui/core": "4.9.3",
    "@material-ui/icons": "^4.9.1",
    "@svgr/webpack": "4.3.3",
    "@tsed/common": "5.42.2",
    "@tsed/core": "5.42.2",
    "@tsed/di": "5.42.2",
    "@tsed/mongoose": "^5.42.2",
    "@tsed/swagger": "5.42.2",
    "@tsed/testing": "5.42.2",
    "@tsed/testing-mongoose": "5.42.2",
    "@types/swagger-schema-official": "2.0.20",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "axios": "^0.19.2",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.1",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "4.17.1",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "hashids": "^2.2.1",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "method-override": "^3.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "mongoose": "^5.9.1",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.4.0",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.4",
    "ts-httpexceptions": "^4.1.0",
    "ts-pnp": "1.1.5",
    "url-loader": "2.3.0",
    "uuid": "^3.4.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.2",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.5",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.17.2",
    "@types/http-proxy": "^1.17.1",
    "@types/mocha": "5.2.7",
    "@types/mongoose": "^5.7.1",
    "@types/node": "12.12.9",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "^7.5.0",
    "@types/sinon-chai": "3.2.3",
    "@types/supertest": "2.0.8",
    "nodemon": "^2.0.2",
    "ts-node": "8.5.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "5.20.1",
    "typescript": "3.7.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
