{
  "name": "shorturl",
  "version": "1.0.0",
  "description": "Generate Short URL",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "install": "concurrently \"cd ./server && npm install && cd ..\" \"cd ./client && npm install && cd ..\"",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "start": "concurrently \"cd ./server && npm run start\"",
    "start:dev": "concurrently \"cd ./server && npm run start:dev\" \"cd ./client && npm run start\"",
    "build:client": "cd ./client && npm run build && cd ..",
    "build:server": "cd ./server && npm run build && cd .."
  },
  "keywords": [
    "Short",
    "URL",
    "React",
    "NestJS"
  ],
  "author": "Ashish Patel <zoom2ashish@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "rimraf": "^3.0.2"
  }
}
