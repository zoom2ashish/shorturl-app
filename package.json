{
  "name": "shorturl",
  "version": "1.0.0",
  "description": "Short URL Generator App",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist server/dist client/build",
    "postinstall": "npm install --prefix server && npm install --prefix client",
    "build": "npm run build:server && npm run build:client",
    "start": "cd ./server && npm run start",
    "start:dev": "concurrently \"cd ./server && npm run start:dev\" \"cd ./client && npm run start\"",
    "build:client": "cd ./client && npm run build",
    "build:server": "cd ./server && npm run build"
  },
  "keywords": [
    "Short",
    "URL",
    "React",
    "NestJS"
  ],
  "author": "Ashish Patel <zoom2ashish@gmail.com>",
  "license": "ISC",
